function apbd_getusersuk() {
	$user =  user_load($GLOBALS['user']->uid);
	
	$pquery = sprintf("select kodesuk from apbdop where username='%s'", $user->name);
	$pres = db_query($pquery);
	if ($data=db_fetch_object($pres)) {
		return $data->kodesuk;
	}
	return '';
}

