function batastgl(){
	
	$sql = 'select tahun, revisi, uraian, tglbatasrka
		from {setupapp}';
        $res = db_query(db_rewrite_sql($sql));
        if ($res) {
            $data = db_fetch_object($res);
			if ($data) {
               $tgltgl		=	$data->tglbatasrka;
                
			} 
        }
	$arrtgl=explode('-',$tgltgl);
	$batas = mktime(6, 0, 0, $arrtgl[1], $arrtgl[2], $arrtgl[0]) ;
	$sekarang = time () ;
	$selisih =($batas-$sekarang) ;
	if($selisih>0){
		return true;
	}
	else{
		return false;
	}
	
	//return true;
}
