function apbd_hitungpersen($awal, $akhir){
	if ($awal == $akhir) 
		$x = 0;
	else if ($awal == 0)
		$x = 100;
	else
		$x = (($akhir - $awal)/$awal)*100;
	
	return $x;
}


function apbd_theme_table($header, $rows, $attributes = array(), $caption = NULL) {


	$output = '<table' . drupal_attributes($attributes) . ">\n";
	
	if (isset($caption)) {
		$output .= '<caption>' . $caption . "</caption>\n";
	}
	
	// Format the table header:
	if (count($header)) {
		// HTML requires that the thead tag has tr tags in it followed by tbody
		// tags. Using ternary operator to check and see if we have any rows.
		$output .= (count($rows) ? ' <thead>' : '');
		
		foreach ($header as $headerrow) {		
			$ts = tablesort_init($headerrow);
			$output .= '<tr>';
			foreach ($headerrow as $cell) {
				$cell = tablesort_header($cell, $headerrow, $ts);
				$output .= _theme_table_cell($cell, TRUE);
			}
			$output .= '</tr>';
		}
		// Using ternary operator to close the tags based on whether or not there are rows
		$output .= (count($rows) ? "</thead>\n" : "\n");
	}
	else {
	  $ts = array();
	}
	
	// Format the table rows:
	if (count($rows)) {
	  $output .= "<tbody>\n";
	  $flip = array(
		'even' => 'odd1',
		'odd' => 'even1',
	  );
	  $class = 'even';
	  foreach ($rows as $number => $row) {
		$attributes = array();
	
		// Check if we're dealing with a simple or complex row
		if (isset($row['data'])) {
		  foreach ($row as $key => $value) {
			if ($key == 'data') {
			  $cells = $value;
			}
			else {
			  $attributes[$key] = $value;
			}
		  }
		}
		else {
		  $cells = $row;
		}
		if (count($cells)) {
		  // Add odd/even class
		  $class = $flip[$class];
		  if (isset($attributes['class'])) {
			$attributes['class'] .= ' ' . $class;
		  }
		  else {
			$attributes['class'] = $class;
		  }
	
		  // Build row
		  $output .= ' <tr' . drupal_attributes($attributes) . '>';
		  $i = 0;
		  foreach ($cells as $cell) {
			$cell = tablesort_cell($cell, $header, $ts, $i++);
			$output .= _theme_table_cell($cell);
		  }
		  $output .= " </tr>\n";
		}
	  }
	  $output .= "</tbody>\n";
	}
	
	$output .= "</table>\n";
	return $output;
}

function apbdIsJenisBerubah($kodekeg, $id) {
	
	$jenisrevisi = 1;
	$sqlkeg = sprintf(' where id=\'%s\'', $id);
	$sql_r = 'select jenisrevisi from kegiatanrevisiperubahan ' . $sqlkeg;
	$res = db_query($sql_r);
	if ($data = db_fetch_object($res)) {
		$jenisrevisi = $data->jenisrevisi;
	}
	
	$strket = '';
	if  (($jenisrevisi==1) or ($jenisrevisi==2)) {
		$gaji = 0; $gajip = 0;
		$pegawai = 0; $pegawaip = 0;
		$barangjasa = 0; $barangjasap = 0;
		$modal = 0; $modalp = 0;

		$sqlkeg = sprintf(' where k.kodekeg=\'%s\'', $kodekeg);

		$sql_r = 'select sum(a.jumlah) perubahan from anggperkegrevisi a inner join kegiatanrevisi k on a.kodekeg=k.kodekeg ' . $sqlkeg;

		//$sql_p = 'select sum(a.jumlahp) penetapan from anggperkegperubahan a inner join kegiatanperubahan k on a.kodekeg=k.kodekeg ' . $sqlkeg;
		
		$sql_p = 'select sum(a.jumlah) penetapan from anggperkeg a inner join kegiatanskpd k on a.kodekeg=k.kodekeg ' . $sqlkeg;

		$jenisberubah = false;

		//Gaji
		$sqlj = " and left(a.kodero, 3)='511'";
		$res = db_query($sql_p . $sqlj );
		if ($data = db_fetch_object($res)) {
			$gaji = $data->penetapan;
		}
		$res = db_query($sql_r . $sqlj );
		if ($data = db_fetch_object($res)) {
			$gajip = $data->perubahan;
		}
		$jenisberubah = ($gaji!=$gajip);

		//drupal_set_message($gaji);
		//drupal_set_message($gajip);
		
		if ($jenisberubah==false) {
			//Pegawai
			$sqlj = " and left(a.kodero, 3)='521'";
			$res = db_query($sql_p . $sqlj );
			if ($data = db_fetch_object($res)) {
				$pegawai = $data->penetapan;
			}
			$res = db_query($sql_r . $sqlj );
			if ($data = db_fetch_object($res)) {
				$pegawaip = $data->perubahan;
			}
			$jenisberubah = ($pegawai!=$pegawaip);
		}
		//drupal_set_message($pegawai);
		//drupal_set_message($pegawaip);

		if ($jenisberubah==false) {
			//Barang Jasa
			$sqlj = " and left(a.kodero, 3)='522'";
			$res = db_query($sql_p . $sqlj );
			if ($data = db_fetch_object($res)) {
				$barangjasa = $data->penetapan;
			}
			$res = db_query($sql_r . $sqlj );
			if ($data = db_fetch_object($res)) {
				$barangjasap = $data->perubahan;
			}
			$jenisberubah = ($barangjasa!=$barangjasap);
		}

		if ($jenisberubah==false) {
			//Modal
			$sqlj = " and left(a.kodero, 3)='523'";
			$res = db_query($sql_p . $sqlj );
			if ($data = db_fetch_object($res)) {
				$modal = $data->penetapan;
			}
			$res = db_query($sql_r . $sqlj );
			if ($data = db_fetch_object($res)) {
				$modalp = $data->perubahan;
			}
			$jenisberubah = ($modal!=$modalp);
		}

		if ($jenisberubah)
			$strket = l('<font color="red">Ada perubahan jenis rekening</font>', 'apbd/kegiatanrevisi/rekening/' . $kodekeg . '/' . $id , array('html'=>TRUE));
		else
			$strket ='Ok';
	}
	return $strket;
}

function createT($header, $row){
	$str='<table class="table table-striped">';
	if(sizeof($header)>0)
		$str.='<thead>';
	for($n=0;$n<sizeof($header);$n++){
		$str.='<tr>';
		for($m=0;$m<sizeof($header[$n]);$m++){
			if(isset($header[$n][$m]['colspan'])){
				$colspan=$header[$n][$m]['colspan'];
			}
			else{
				$colspan='';
			}
			if(isset($header[$n][$m]['rowspan']) && $header[$n][$m]['rowspan']!=null){
				$rowspan=$header[$n][$m]['rowspan'];
			}
			else{
				$rowspan='';
			}
			
			$str.='<th  align="center" rowspan="'.$header[$n][$m]['rowspan'].'" style="'.$header[$n][$m]['style'].'" width="'.$header[$n][$m]['width'].'">' . $header[$n][$m]['data'] . '</th>';
		}
		$str.='</tr>';
		
	}
	if(sizeof($header)>0)
		$str.='</thead>';
	/*
	$str='<table><th><td>a</td>';
	$str.='<td>b</td></th></table>';

	$str.='<td>b</td></th></table>';*/
	for($n=0;$n<sizeof($row);$n++)
	{
		$str.='<tr >';
		for($m=0;$m<sizeof($row[$n]);$m++)
		{
			$str.='<td width="'.$row[$n][$m]['width'].'" style="'.$row[$n][$m]['style'].'" align="'.$row[$n][$m]['align'].'">'.$row[$n][$m]['data'].'</td>';
		}
		$str.='</tr>';
	}
	$str.='</table>';
	return $str;
}

